{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "lib": [
            "es2020"
        ],
        "outDir": ".",
        "rootDir": ".",
        "strict": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "declaration": false
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}